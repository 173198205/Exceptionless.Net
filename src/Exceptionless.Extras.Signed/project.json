            }
        },
        "net45": {
            "buildOptions": {
                "define": [ "EMBEDDED", "NET45" ]
            },
            "frameworkAssemblies": {
                "Microsoft.VisualBasic": "", 
                "System.Configuration": "",
                "System.Net.Http": ""
            }
        }
    },
    "supports": { }
