        "repository": {
            "type": "git",
            "url": "https://github.com/exceptionless/Exceptionless.Net"
        },
        "files": {
            "mappings": {
                "tools/": "NuGet/tools/*"
            },
            "include": [
                "readme.txt"
            ]
        }
    },
    "buildOptions": {
        "compile": [
            "**/*.cs",
            "../GlobalAssemblyInfo.cs"
        ],
        "embed": {
            "mappings": {
                "Exceptionless.Json.Dynamic.snk": "Newtonsoft.Json/Dynamic.snk"
            }
        },
        "nowarn": [
            "CS1591"
        ],
        "optimize": true,
        "warningsAsErrors": true,
        "xmlDoc": true
    },
